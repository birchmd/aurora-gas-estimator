{
  "statements": [
    {
      "Assign": {
        "name": "source",
        "expression": {
          "CreateAccount": {
            "initial_balance": "0xffffffff",
            "secret_key": null,
            "initial_nonce": null
          }
        }
      }
    },
    {
      "Assign": {
        "name": "dest",
        "expression": {
          "Primitive": {
            "Bytes": "0x000000000000000000beef000000000000000000"
          }
        }
      }
    },
    {
      "Assign": {
        "name": "transfer",
        "expression": {
          "CallContract": {
            "contract": "dest",
            "signing_account": "source",
            "data": null,
            "value": "0x00000fff"
          }
        }
      }
    },
    {
      "Assign": {
        "name": "source_expected_balance",
        "expression": {
          "Primitive": {
            "U256": "0xfffff000"
          }
        }
      }
    },
    {
      "Assign": {
        "name": "source_actual_balance",
        "expression": {
          "GetBalance": {
            "address": "source"
          }
        }
      }
    },
    {
      "AssertEq": {
        "left": "source_expected_balance",
        "right": "source_actual_balance"
      }
    },
    {
      "Assign": {
        "name": "source_expected_nonce",
        "expression": {
          "Primitive": {
            "U256": "0x01"
          }
        }
      }
    },
    {
      "Assign": {
        "name": "source_actual_nonce",
        "expression": {
          "GetNonce": {
            "address": "source"
          }
        }
      }
    },
    {
      "AssertEq": {
        "left": "source_expected_nonce",
        "right": "source_actual_nonce"
      }
    },
    {
      "Assign": {
        "name": "dest_expected_balance",
        "expression": {
          "Primitive": {
            "U256": "0x00000fff"
          }
        }
      }
    },
    {
      "Assign": {
        "name": "dest_actual_balance",
        "expression": {
          "GetBalance": {
            "address": "dest"
          }
        }
      }
    },
    {
      "AssertEq": {
        "left": "dest_expected_balance",
        "right": "dest_actual_balance"
      }
    }
  ]
}
